{
  "name": "vue3-vite-ts-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "preview": "vite preview",
    "build:report": "vue-tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,vue}\"",
    "typecheck": "vue-tsc --noEmit --skipLibCheck -p tsconfig.app.json"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^13.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vite-plugin-eslint2": "^5.0.3",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.17",
    "@types/nprogress": "^0.2.3",
    "@types/postcss-pxtorem": "^6.1.0",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "less": "^4.3.0",
    "node-modules-inspector": "^0.6.8",
    "postcss": "^8.5.3",
    "postcss-pxtorem": "^6.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "unocss": "66.1.1",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-simple-html": "^0.2.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  },
  "volta": {
    "node": "22.14.0"
  }
}
